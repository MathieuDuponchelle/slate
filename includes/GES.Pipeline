<pre><code class="highlight python"><span class="nd">@accepts</span><span class="p">(</span><span class="n">GES</span><span class="o">.</span><span class="n">Pipeline</span><span class="p">,</span> <span class="n">GES</span><span class="o">.</span><span class="n">PipelineFlags</span><span class="p">)</span>
